.footer
	display flex
	align-items center
	min-height 62px
	font-size 14px
	line-height 21px
	text-align left
	letter-spacing -.2px
	color rgb(255, 255, 255)
	background-color rgb(147, 185, 66)
	.container_flex_true
		box-sizing border-box
		align-items end
		justify-content space-between
		padding-top 11px
		height 100%
		&:after
			display none
	&__text
		max-width 86%
	&__logo
		display flex
		align-items center
		span
			padding-left 17px
	&__social
		height 100%
		.social__resource
			margin-bottom 0
			margin-left 17px
		.social
			display flex
			align-items center
			height 100%
